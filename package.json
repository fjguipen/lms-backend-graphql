{
  "name": "z1-senior-backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "dc:up": "docker-compose up -d",
    "dc:recreate": "docker-compose up -d --force-recreate",
    "dc:logs": "docker-compose logs -f",
    "db:init": "node ./build/db/scripts/initDB.js",
    "clean-deps": "rm -r ./node_modules",
    "clean": "rm -rf ./build/*",
    "start": "node build/src/server.js",
    "build-watch": "tsc -w --preserveWatchOutput",
    "serve-dev": "nodemon -r source-map-support/register --watch build ./build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run build-watch\" \"npm run serve-dev\"",
    "deploy-dev": "npm i && npm run clean && tsc && npm run dc:up",
    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.0.18",
    "@graphql-tools/merge": "^6.0.18",
    "apollo-server": "^2.16.1",
    "apollo-server-express": "^2.16.1",
    "connect-pg-simple": "^6.1.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "knex": "^0.21.2",
    "objection": "^2.2.2",
    "pg": "^8.3.0",
    "prettier": "^2.0.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "^14.5.0",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "source-map-support": "^0.5.19",
    "typescript": "^3.9.7",
    "ts-node": "^8.10.2"
  }
}
